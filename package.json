{
  "name": "@nestjslatam/ddd-valueobjects",
  "version": "1.0.0",
  "description": "Domain-Driven Design Value Objects library for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "nest build ddd-valueobjects",
    "build:watch": "nest build ddd-valueobjects --watch",
    "start": "nest start example-app",
    "start:dev": "nest start example-app --watch",
    "start:debug": "nest start example-app --debug --watch",
    "start:prod": "node dist/apps/example-app/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "eslint \"{libs,apps}/**/*.ts\" --fix",
    "format": "prettier --write \"{libs,apps}/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "release": "standard-version"
  },
  "keywords": [
    "nestjs",
    "ddd",
    "domain-driven-design",
    "value-objects",
    "typescript",
    "nestjslatam"
  ],
  "author": "NestJS LATAM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjslatam/ddd-valueobjects"
  },
  "bugs": {
    "url": "https://github.com/nestjslatam/ddd-valueobjects/issues"
  },
  "homepage": "https://github.com/nestjslatam/ddd-valueobjects#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/testing": "^11.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/index.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@nestjslatam/ddd-valueobjects(|/.*)$": "<rootDir>/libs/ddd-valueobjects/src/$1"
    }
  },
  "dependencies": {
    "@nestjslatam/ddd-lib": "^2.0.0"
  }
}
